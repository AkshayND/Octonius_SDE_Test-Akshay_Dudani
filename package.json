    "@types/node": "^9.6.42",
    "angular-router-loader": "^0.8.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^4.0.1",
    "del-cli": "^1.1.0",
    "html-loader": "^0.5.5",
    "ngtemplate-loader": "^2.0.1",
    "raw-loader": "^0.5.1",
    "ts-loader": "^6.0.1",
    "typescript": "^2.9.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.1.1",
    "webpack-merge": "^4.2.1"
  }
